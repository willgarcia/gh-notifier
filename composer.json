{
    "name": "wooshell/gh-notifier",
    "description": "Github releases notifier",
    "keywords": ["github","notification", "store"],
    "type": "project",
    "description": "Tracks github projects to let you know when new releases are available.",
    "license": "MIT",
    "authors": [
        {
            "name": "William Garcia",
            "email": "garcia.rodriguez.william@gmail.com"
        }
    ],
    "require": {
        "knplabs/github-api": "*",
        "symfony/console": "2.2.*",
        "swiftmailer/swiftmailer": "5.1.*@dev",
        "symfony/yaml": "2.3.*",
        "wooshell/store-notifier": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "": "src" }
    },
    "bin": ["bin/gh-notifier"],
    "config": {
        "bin-dir": "bin"
    }
}
